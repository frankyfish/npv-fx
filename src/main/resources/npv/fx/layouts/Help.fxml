                        <ImageView fitHeight="1577.0" fitWidth="625.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../images/algorithm2.jpg" />
                           </image>
                        </ImageView>
                     </content>
                  </ScrollPane>
               </content>
          </Tab>
            <Tab text="Algorithm III">
               <content>
                  <ScrollPane prefHeight="200.0" prefWidth="200.0">
                     <content>
                        <ImageView fitHeight="1071.0" fitWidth="294.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../images/algorithm3.jpg" />
                           </image>
                        </ImageView>
                     </content>
                  </ScrollPane>
               </content>
