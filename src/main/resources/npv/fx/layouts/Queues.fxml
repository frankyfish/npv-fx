<AnchorPane id="AnchorPane" prefHeight="625.0" prefWidth="978.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="npv.fx.FXMLDocumentController">
    <children>
        <ToolBar prefHeight="40.0" prefWidth="978.0">
            <items>
                <Button fx:id="returnToRoot" mnemonicParsing="false" onAction="#handleControlButtonAction" text="Back" />
                <!--used to move btns to the right 
                http://stackoverflow.com/questions/24896498/how-to-right-allign-a-button-in-java-fx-toolbar
                -->
                <Pane prefHeight="28.0" prefWidth="56.0" HBox.hgrow="ALWAYS" />
            </items>
        </ToolBar>
      <TableView fx:id="miniProjectTable" layoutX="35.0" layoutY="67.0" nodeOrientation="LEFT_TO_RIGHT" prefHeight="376.0" prefWidth="453.0">
        <columns>
          <TableColumn fx:id="periodI" prefWidth="75.0" text="i" />
          <TableColumn fx:id="time" prefWidth="75.0" text="t" />
            <TableColumn fx:id="income" prefWidth="75.0" text="D" />
            <TableColumn fx:id="gain" prefWidth="75.0" text="C" />
            <TableColumn fx:id="directExpense" prefWidth="75.0" text="S" />
            <TableColumn fx:id="factorK" prefWidth="75.0" text="k" />
        </columns>
      </TableView>
      <TextField fx:id="tfT" layoutX="122.0" layoutY="454.0" prefHeight="26.0" prefWidth="58.0" promptText="t" />
      <TextField fx:id="tfD" layoutX="197.0" layoutY="454.0" prefHeight="26.0" prefWidth="58.0" promptText="D" />
      <TextField fx:id="tfC" layoutX="272.0" layoutY="454.0" prefHeight="26.0" prefWidth="58.0" promptText="C" />
      <Button fx:id="btnAdd" layoutX="346.0" layoutY="454.0" mnemonicParsing="false" onAction="#handleQueueActions" text="Add" />
      <Button fx:id="btnCountFactorK" layoutX="432.0" layoutY="557.0" mnemonicParsing="false" onAction="#handleQueueActions" text="Count" />
      <TextField fx:id="tfPercentQueue" layoutX="198.0" layoutY="518.0" prefHeight="26.0" prefWidth="57.0" promptText="\%" />
      <Button fx:id="testBtn" layoutX="264.0" layoutY="518.0" mnemonicParsing="false" onAction="#handleQueueActions" text="TestData" />
      <TextArea fx:id="taQueues" editable="false" layoutX="515.0" layoutY="67.0" prefHeight="64.0" prefWidth="411.0" promptText="Results of sorting will be here" />
      <ChoiceBox fx:id="cbAlgorithmSelection" layoutX="197.0" layoutY="557.0" prefWidth="150.0" />
      <Separator layoutX="63.0" layoutY="504.0" prefHeight="0.0" prefWidth="862.0" />
      <Text layoutX="146.0" layoutY="535.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Alpha" />
      <Text layoutX="140.0" layoutY="576.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Sort by" />
    </children>
</AnchorPane>
