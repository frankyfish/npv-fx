      <Button fx:id="btnAdd" layoutX="289.0" layoutY="454.0" mnemonicParsing="false" onAction="#handleQueueActions" text="Add" />
      <Button fx:id="btnCountFactorK" layoutX="372.0" layoutY="454.0" mnemonicParsing="false" onAction="#handleQueueActions" text="Count" />
      <TextField fx:id="tfPercentQueue" layoutX="59.0" layoutY="491.0" prefHeight="26.0" prefWidth="57.0" promptText="\%" />
      <GridPane gridLinesVisible="true" layoutX="616.0" layoutY="91.0">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
      </GridPane>
      <Button fx:id="testBtn" layoutX="289.0" layoutY="491.0" mnemonicParsing="false" onAction="#handleQueueActions" text="TestData" />
    </children>
</AnchorPane>
